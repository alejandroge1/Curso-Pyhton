def colores(*args):
	pass

colores('rojo', 'azul', 'verde', 'amarillo') #4
colores('lila', 'negro', 'rojo') #3
colores('rosa') #1
colores('marrón', 'naranja') #2

def colores(*args):
	print('El color', args[1], 'es mi favorito.', 'El color', args[0], 'tampoco está mal.')

colores('negro','rojo')

def suma(*args):
    res= args[0] + args[1] + args[2] + args[3] + args[4]
    print('R= ',res)
    
suma(1,2,3,4,5)
